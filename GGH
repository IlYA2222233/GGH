-- ========== –ü–õ–ê–í–ù–û–ï –ó–ê–ö–†–´–¢–ò–ï ==========
CloseButton.MouseButton1Click:Connect(function()
    -- –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–∫—Ä—ã—Ç–∏—è
    local closeTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    })
    
    local fadeTween = TweenService:Create(MainFrame, TweenInfo.new(0.3), {
        BackgroundTransparency = 1
    })
    
    closeTween:Play()
    fadeTween:Play()
    
    closeTween.Completed:Wait()
    ScreenGui:Destroy()
end)

-- ========== –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò ==========

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –Ω–æ–≤–æ–π –∫–Ω–æ–ø–∫–∏ –≤ —Ä–∞–∑–¥–µ–ª (–º–æ–∂–Ω–æ –≤—ã–∑—ã–≤–∞—Ç—å –∏–∑–≤–Ω–µ)
function addButtonToSection(sectionName, buttonText, description, callback)
    local frame = ContentFrames[sectionName]
    if not frame then return end
    
    -- –ù–∞–π—Ç–∏ –ø–æ—Å–ª–µ–¥–Ω—é—é –ø–æ–∑–∏—Ü–∏—é
    local lastPosition = 0
    for _, child in pairs(frame:GetChildren()) do
        if child:IsA("Frame") then
            lastPosition = math.max(lastPosition, child.Position.Y.Offset)
        end
    end
    
    local newButton = createSectionButton(frame, buttonText, description, lastPosition + 70, callback)
    frame.CanvasSize = UDim2.new(0, 0, 0, lastPosition + 140)
    
    return newButton
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–∫—Ä—ã—Ç–∏—è/–ø–æ–∫–∞–∑–∞ GUI
function toggleGUI()
    if MainFrame.Visible then
        TweenService:Create(MainFrame, TweenInfo.new(0.3), {
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0),
            BackgroundTransparency = 1
        }):Play()
        MainFrame.Visible = false
    else
        MainFrame.Visible = true
        TweenService:Create(MainFrame, TweenInfo.new(0.3), {
            Size = UDim2.new(0, GUI_SETTINGS.Width, 0, GUI_SETTINGS.Height),
            Position = UDim2.new(0.5, -GUI_SETTINGS.Width/2, 0.5, -GUI_SETTINGS.Height/2),
            BackgroundTransparency = 0
        }):Play()
    end
end

-- –ì–æ—Ä—è—á–∞—è –∫–ª–∞–≤–∏—à–∞ –¥–ª—è –ø–æ–∫–∞–∑–∞/—Å–∫—Ä—ã—Ç–∏—è GUI
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.RightControl then
        toggleGUI()
    end
end)

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –æ—á–∏—Å—Ç–∫–∞ –ø—Ä–∏ –≤—ã—Ö–æ–¥–µ
game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == LocalPlayer then
        -- –û—Ç–∫–ª—é—á–∞–µ–º –≤—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ –ø—Ä–∏ –≤—ã—Ö–æ–¥–µ
        if Functions.Fly.Enabled then toggleFly() end
        if Functions.ESP.Enabled then toggleESP() end
        if Functions.Names.Enabled then toggleNames() end
        if Functions.NightVision.Enabled then toggleNightVision() end
        if Functions.XRay.Enabled then toggleXRay() end
        if Functions.Noclip.Enabled then toggleNoclip() end
    end
end)

print("üéÆ GGH GUI —É—Å–ø–µ—à–Ω–æ –∑–∞–≥—Ä—É–∂–µ–Ω!")
print("üìå –ù–∞–∂–º–∏—Ç–µ RightControl –¥–ª—è –ø–æ–∫–∞–∑–∞/—Å–∫—Ä—ã—Ç–∏—è")
print("‚ö° –í—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ –≥–æ—Ç–æ–≤—ã –∫ —Ä–∞–±–æ—Ç–µ!")
print("üîß –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ addButtonToSection() –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —Å–≤–æ–∏—Ö –∫–Ω–æ–ø–æ–∫")
