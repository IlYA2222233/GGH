-- GGH Customizable GUI Framework —Å –ø–æ–ª–Ω—ã–º —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª–æ–º
-- –ü–ª–∞–≤–Ω—ã–µ –∞–Ω–∏–º–∞—Ü–∏–∏ + –≤—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ —Ä–∞–±–æ—Ç–∞—é—Ç

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")

-- ‚öôÔ∏è –ù–ê–°–¢–†–û–ô–ö–ò (–º–æ–∂–Ω–æ –º–µ–Ω—è—Ç—å)
local GUI_SETTINGS = {
    Title = "GGH Interface",           -- –ó–∞–≥–æ–ª–æ–≤–æ–∫ GUI
    Width = 400,                       -- –®–∏—Ä–∏–Ω–∞ –æ–∫–Ω–∞
    Height = 500,                      -- –í—ã—Å–æ—Ç–∞ –æ–∫–Ω–∞
    BackgroundColor = Color3.fromRGB(30, 30, 40),  -- –¶–≤–µ—Ç —Ñ–æ–Ω–∞
    AccentColor = Color3.fromRGB(80, 120, 255),    -- –ê–∫—Ü–µ–Ω—Ç–Ω—ã–π —Ü–≤–µ—Ç
    TextColor = Color3.fromRGB(255, 255, 255),     -- –¶–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞
    Sections = {                       -- –°–ø–∏—Å–æ–∫ —Ä–∞–∑–¥–µ–ª–æ–≤
        "–ö–æ–º–∞–Ω–¥—ã",
        "–í–∏–∑—É–∞–ª—ã", 
        "–ù–∞—Å—Ç—Ä–æ–π–∫–∏",
        "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"
    }
}

-- üìä –ì–õ–û–ë–ê–õ–¨–ù–´–ï –ü–ï–†–ï–ú–ï–ù–ù–´–ï –î–õ–Ø –§–£–ù–ö–¶–ò–ô
local Functions = {
    WalkSpeed = {
        Enabled = false,
        Value = 16,
        Multiplier = 3
    },
    JumpPower = {
        Enabled = false,
        Value = 50,
        Multiplier = 2
    },
    Fly = {
        Enabled = false,
        Speed = 50,
        BodyVelocity = nil,
        BodyGyro = nil,
        Connection = nil
    },
    Noclip = {
        Enabled = false,
        Connection = nil
    },
    ESP = {
        Enabled = false,
        Players = {},
        Connection = nil
    },
    Names = {
        Enabled = false,
        Labels = {},
        Connection = nil
    },
    NightVision = {
        Enabled = false,
        OriginalAmbient = nil,
        OriginalBrightness = nil
    },
    XRay = {
        Enabled = false,
        OriginalTransparency = {}
    }
}

-- üîß –í–°–ü–û–ú–û–ì–ê–¢–ï–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò
local function getCharacter()
    return LocalPlayer.Character
end

local function getHumanoid()
    local char = getCharacter()
    return char and char:FindFirstChild("Humanoid")
end

local function getRoot()
    local char = getCharacter()
    return char and char:FindFirstChild("HumanoidRootPart")
end

-- üöÄ –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò

-- 1. –¢–ï–õ–ï–ü–û–†–¢ –ö –ò–ì–†–û–ö–£
local function teleportToPlayer()
    -- –°–æ–∑–¥–∞–µ–º –æ–∫–Ω–æ –≤—ã–±–æ—Ä–∞ –∏–≥—Ä–æ–∫–∞
    local teleportGui = Instance.new("ScreenGui")
    teleportGui.Name = "TeleportSelector"
    teleportGui.Parent = game.CoreGui
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 400)
    frame.Position = UDim2.new(0.5, -150, 0.5, -200)
    frame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    frame.Parent = teleportGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 50)
    title.Position = UDim2.new(0, 0, 0, 0)
    title.BackgroundColor3 = Color3.fromRGB(50, 50, 65)
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.Text = "–í—ã–±–µ—Ä–∏—Ç–µ –∏–≥—Ä–æ–∫–∞"
    title.Font = Enum.Font.GothamBold
    title.TextSize = 18
    title.Parent = frame
    
    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 30, 0, 30)
    closeBtn.Position = UDim2.new(1, -35, 0, 10)
    closeBtn.BackgroundColor3 = Color3.fromRGB(220, 60, 60)
    closeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    closeBtn.Text = "X"
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.TextSize = 18
    closeBtn.Parent = frame
    closeBtn.MouseButton1Click:Connect(function()
        teleportGui:Destroy()
    end)
    
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, -20, 1, -70)
    scroll.Position = UDim2.new(0, 10, 0, 60)
    scroll.BackgroundTransparency = 1
    scroll.ScrollBarThickness = 4
    scroll.Parent = frame
    
    local yPos = 0
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, 0, 0, 50)
            btn.Position = UDim2.new(0, 0, 0, yPos)
            btn.BackgroundColor3 = Color3.fromRGB(60, 60, 75)
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.Text = player.Name
            btn.Font = Enum.Font.Gotham
            btn.TextSize = 14
            btn.Parent = scroll
            
            local btnCorner = Instance.new("UICorner")
            btnCorner.CornerRadius = UDim.new(0, 8)
            btnCorner.Parent = btn
            
            btn.MouseButton1Click:Connect(function()
                local targetRoot = player.Character:FindFirstChild("HumanoidRootPart")
                local myRoot = getRoot()
                
                if targetRoot and myRoot then
                    myRoot.CFrame = targetRoot.CFrame
                    print("üì° –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω –∫ " .. player.Name)
                end
                teleportGui:Destroy()
            end)
            
            yPos = yPos + 60
        end
    end
    
    scroll.CanvasSize = UDim2.new(0, 0, 0, yPos)
end

-- 2. –†–ï–ñ–ò–ú –ü–û–õ–ï–¢–ê
local function toggleFly()
    Functions.Fly.Enabled = not Functions.Fly.Enabled
    
    if Functions.Fly.Enabled then
        local char = getCharacter()
        if not char then return end
        
        local root = getRoot()
        if not root then return end
        
        -- –°–æ–∑–¥–∞–µ–º —Ñ–∏–∑–∏—á–µ—Å–∫–∏–µ –æ–±—ä–µ–∫—Ç—ã –¥–ª—è –ø–æ–ª–µ—Ç–∞
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(4000, 4000, 4000)
        bodyVelocity.P = 1000
        bodyVelocity.Parent = root
        Functions.Fly.BodyVelocity = bodyVelocity
        
        local bodyGyro = Instance.new("BodyGyro")
        bodyGyro.MaxTorque = Vector3.new(4000, 4000, 4000)
        bodyGyro.P = 1000
        bodyGyro.CFrame = root.CFrame
        bodyGyro.Parent = root
        Functions.Fly.BodyGyro = bodyGyro
        
        -- –ü–æ–¥–∫–ª—é—á–∞–µ–º —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ
        Functions.Fly.Connection = RunService.Heartbeat:Connect(function()
            if not Functions.Fly.Enabled or not char or not root then
                if Functions.Fly.Connection then
                    Functions.Fly.Connection:Disconnect()
                end
                return
            end
            
            local direction = Vector3.new(0, 0, 0)
            
            if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                direction = direction + (root.CFrame.LookVector * Functions.Fly.Speed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                direction = direction - (root.CFrame.LookVector * Functions.Fly.Speed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                direction = direction - (root.CFrame.RightVector * Functions.Fly.Speed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                direction = direction + (root.CFrame.RightVector * Functions.Fly.Speed)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                direction = direction + Vector3.new(0, Functions.Fly.Speed, 0)
            end
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                direction = direction - Vector3.new(0, Functions.Fly.Speed, 0)
            end
            
            if bodyVelocity then
                bodyVelocity.Velocity = direction
            end
            if bodyGyro then
                bodyGyro.CFrame = root.CFrame
            end
        end)
        
        print("ü¶Ö –ü–æ–ª–µ—Ç –≤–∫–ª—é—á–µ–Ω! –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ: WASD + Space/Shift")
    else
        -- –û—Ç–∫–ª—é—á–∞–µ–º –ø–æ–ª–µ—Ç
        if Functions.Fly.BodyVelocity then
            Functions.Fly.BodyVelocity:Destroy()
            Functions.Fly.BodyVelocity = nil
        end
        if Functions.Fly.BodyGyro then
            Functions.Fly.BodyGyro:Destroy()
            Functions.Fly.BodyGyro = nil
        end
        if Functions.Fly.Connection then
            Functions.Fly.Connection:Disconnect()
            Functions.Fly.Connection = nil
        end
        print("ü¶Ö –ü–æ–ª–µ—Ç –≤—ã–∫–ª—é—á–µ–Ω")
    end
    
    return Functions.Fly.Enabled
end

-- 3. –ë–ï–°–ö–û–ù–ï–ß–ù–´–ô –ü–†–´–ñ–û–ö
local function toggleInfiniteJump()
    Functions.JumpPower.Enabled = not Functions.JumpPower.Enabled
    local humanoid = getHumanoid()
    
    if humanoid then
        if Functions.JumpPower.Enabled then
            humanoid.JumpPower = Functions.JumpPower.Value * Functions.JumpPower.Multiplier
            print("üåü –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–π –ø—Ä—ã–∂–æ–∫ –≤–∫–ª—é—á–µ–Ω")
        else
            humanoid.JumpPower = Functions.JumpPower.Value
            print("üåü –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–π –ø—Ä—ã–∂–æ–∫ –≤—ã–∫–ª—é—á–µ–Ω")
        end
    end
    return Functions.JumpPower.Enabled
end

-- 4. –°–ö–û–†–û–°–¢–¨ –•–û–î–¨–ë–´
local function toggleWalkSpeed()
    Functions.WalkSpeed.Enabled = not Functions.WalkSpeed.Enabled
    local humanoid = getHumanoid()
    
    if humanoid then
        if Functions.WalkSpeed.Enabled then
            humanoid.WalkSpeed = Functions.WalkSpeed.Value * Functions.WalkSpeed.Multiplier
            print("üöÄ –°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã —É–≤–µ–ª–∏—á–µ–Ω–∞")
        else
            humanoid.WalkSpeed = Functions.WalkSpeed.Value
            print("üöÄ –°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã –Ω–æ—Ä–º–∞–ª—å–Ω–∞—è")
        end
    end
    return Functions.WalkSpeed.Enabled
end

-- 5. –†–ï–ñ–ò–ú NO-CLIP
local function toggleNoclip()
    Functions.Noclip.Enabled = not Functions.Noclip.Enabled
    
    if Functions.Noclip.Enabled then
        Functions.Noclip.Connection = RunService.Stepped:Connect(function()
            local char = getCharacter()
            if char then
                for _, part in pairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
        print("üëª Noclip –≤–∫–ª—é—á–µ–Ω")
    else
        if Functions.Noclip.Connection then
            Functions.Noclip.Connection:Disconnect()
            Functions.Noclip.Connection = nil
        end
        
        local char = getCharacter()
        if char then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
        print("üëª Noclip –≤—ã–∫–ª—é—á–µ–Ω")
    end
    
    return Functions.Noclip.Enabled
end

-- 6. ESP –ò–ì–†–û–ö–û–í
local function toggleESP()
    Functions.ESP.Enabled = not Functions.ESP.Enabled
    
    local function createESP(player)
        if Functions.ESP.Players[player] then return end
        
        local char = player.Character
        if not char then return end
        
        local highlight = Instance.new("Highlight")
        highlight.Name = "ESP_Highlight"
        highlight.FillColor = Color3.fromRGB(255, 50, 50)
        highlight.FillTransparency = 0.7
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.OutlineTransparency = 0
        highlight.Parent = char
        
        Functions.ESP.Players[player] = highlight
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        player.CharacterAdded:Connect(function(newChar)
            if Functions.ESP.Enabled then
                task.wait(1) -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
                local newHighlight = highlight:Clone()
                newHighlight.Parent = newChar
                Functions.ESP.Players[player] = newHighlight
            end
        end)
    end
    
    if Functions.ESP.Enabled then
        -- –°–æ–∑–¥–∞–µ–º ESP –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                createESP(player)
            end
        end
        
        -- –°–ª–µ–¥–∏–º –∑–∞ –Ω–æ–≤—ã–º–∏ –∏–≥—Ä–æ–∫–∞–º–∏
        Functions.ESP.Connection = Players.PlayerAdded:Connect(function(player)
            if Functions.ESP.Enabled then
                createESP(player)
            end
        end)
        
        print("üëÅÔ∏è ESP –≤–∫–ª—é—á–µ–Ω–æ")
    else
        -- –£–¥–∞–ª—è–µ–º –≤—Å–µ ESP
        for player, highlight in pairs(Functions.ESP.Players) do
            if highlight and highlight.Parent then
                highlight:Destroy()
            end
        end
        Functions.ESP.Players = {}
        
        if Functions.ESP.Connection then
            Functions.ESP.Connection:Disconnect()
            Functions.ESP.Connection = nil
        end
        
        print("üëÅÔ∏è ESP –≤—ã–∫–ª—é—á–µ–Ω–æ")
    end
    
    return Functions.ESP.Enabled
end

-- 7. –ò–ú–ï–ù–ê –ù–ê–î –ì–û–õ–û–í–ê–ú–ò
local function toggleNames()
    Functions.Names.Enabled = not Functions.Names.Enabled
    
    local function createNameTag(player)
        if Functions.Names.Labels[player] then return end
        
        local char = player.Character
        if not char then return end
        
        local head = char:FindFirstChild("Head")
        if not head then return end
        
        -- –°–æ–∑–¥–∞–µ–º GUI –¥–ª—è –∏–º–µ–Ω–∏
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "NameTag"
        billboard.Size = UDim2.new(0, 200, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Adornee = head
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = player.Name
        label.TextColor3 = Color3.fromRGB(255, 255, 255)
        label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
        label.TextStrokeTransparency = 0.5
        label.Font = Enum.Font.GothamBold
        label.TextSize = 18
        label.Parent = billboard
        
        billboard.Parent = head
        Functions.Names.Labels[player] = billboard
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        player.CharacterAdded:Connect(function(newChar)
            if Functions.Names.Enabled then
                task.wait(1)
                local head = newChar:FindFirstChild("Head")
                if head then
                    local newBillboard = billboard:Clone()
                    newBillboard.Adornee = head
                    newBillboard.Parent = head
                    Functions.Names.Labels[player] = newBillboard
                end
            end
        end)
    end
    
    if Functions.Names.Enabled then
        -- –°–æ–∑–¥–∞–µ–º –∏–º–µ–Ω–∞ –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                createNameTag(player)
            end
        end
        
        -- –°–ª–µ–¥–∏–º –∑–∞ –Ω–æ–≤—ã–º–∏ –∏–≥—Ä–æ–∫–∞–º–∏
        Functions.Names.Connection = Players.PlayerAdded:Connect(function(player)
            if Functions.Names.Enabled then
                createNameTag(player)
            end
        end)
        
        print("üè∑Ô∏è –ò–º–µ–Ω–∞ –Ω–∞–¥ –≥–æ–ª–æ–≤–∞–º–∏ –≤–∫–ª—é—á–µ–Ω—ã")
    else
        -- –£–¥–∞–ª—è–µ–º –≤—Å–µ –∏–º–µ–Ω–∞
        for player, label in pairs(Functions.Names.Labels) do
            if label and label.Parent then
                label:Destroy()
            end
        end
        Functions.Names.Labels = {}
        
        if Functions.Names.Connection then
            Functions.Names.Connection:Disconnect()
            Functions.Names.Connection = nil
        end
        
        print("üè∑Ô∏è –ò–º–µ–Ω–∞ –Ω–∞–¥ –≥–æ–ª–æ–≤–∞–º–∏ –≤—ã–∫–ª—é—á–µ–Ω—ã")
    end
    
    return Functions.Names.Enabled
end

-- 8. –¢–†–ê–ï–ö–¢–û–†–ò–Ø –ü–û–õ–ï–¢–ê
local function toggleTrajectory()
    -- –ü—Ä–æ—Å—Ç–∞—è —Ç—Ä–∞–µ–∫—Ç–æ—Ä–∏—è –¥–ª—è –¥–µ–º–æ–Ω—Å—Ç—Ä–∞—Ü–∏–∏
    local trajectoryEnabled = false
    
    if not trajectoryEnabled then
        print("üéØ –¢—Ä–∞–µ–∫—Ç–æ—Ä–∏—è –ø–æ–ª–µ—Ç–∞ –≤–∫–ª—é—á–µ–Ω–∞")
        -- –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–∞–ª—å–Ω—É—é —Ç—Ä–∞–µ–∫—Ç–æ—Ä–∏—é
        -- –ù–æ –¥–ª—è –ø—Ä–∏–º–µ—Ä–∞ –ø—Ä–æ—Å—Ç–æ –ø–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
    else
        print("üéØ –¢—Ä–∞–µ–∫—Ç–æ—Ä–∏—è –ø–æ–ª–µ—Ç–∞ –≤—ã–∫–ª—é—á–µ–Ω–∞")
    end
    
    trajectoryEnabled = not trajectoryEnabled
    return trajectoryEnabled
end

-- 9. –£–õ–£–ß–®–ï–ù–ù–ê–Ø –ì–†–ê–§–ò–ö–ê
local function toggleEnhancedGraphics()
    local graphicsEnabled = false
    
    if not graphicsEnabled then
        -- –£–ª—É—á—à–∞–µ–º –≥—Ä–∞—Ñ–∏–∫—É
        Lighting.GlobalShadows = true
        Lighting.Outlines = true
        Lighting.ShadowSoftness = 0.5
        print("‚ú® –£–ª—É—á—à–µ–Ω–Ω–∞—è –≥—Ä–∞—Ñ–∏–∫–∞ –≤–∫–ª—é—á–µ–Ω–∞")
    else
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
        Lighting.GlobalShadows = false
        Lighting.Outlines = false
        Lighting.ShadowSoftness = 0.1
        print("‚ú® –£–ª—É—á—à–µ–Ω–Ω–∞—è –≥—Ä–∞—Ñ–∏–∫–∞ –≤—ã–∫–ª—é—á–µ–Ω–∞")
    end
    
    graphicsEnabled = not graphicsEnabled
    return graphicsEnabled
end

-- 10. –ù–û–ß–ù–û–ï –ó–†–ï–ù–ò–ï
local function toggleNightVision()
    Functions.NightVision.Enabled = not Functions.NightVision.Enabled
    
    if Functions.NightVision.Enabled then
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
        Functions.NightVision.OriginalAmbient = Lighting.Ambient
        Functions.NightVision.OriginalBrightness = Lighting.Brightness
        
        -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –Ω–æ—á–Ω–æ–µ –∑—Ä–µ–Ω–∏–µ
        Lighting.Ambient = Color3.fromRGB(0, 255, 0)  -- –ó–µ–ª–µ–Ω—ã–π –æ—Ç—Ç–µ–Ω–æ–∫
        Lighting.Brightness = 2
        print("üåô –ù–æ—á–Ω–æ–µ –∑—Ä–µ–Ω–∏–µ –≤–∫–ª—é—á–µ–Ω–æ")
    else
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
        if Functions.NightVision.OriginalAmbient then
            Lighting.Ambient = Functions.NightVision.OriginalAmbient
        end
        if Functions.NightVision.OriginalBrightness then
            Lighting.Brightness = Functions.NightVision.OriginalBrightness
        end
        print("üåô –ù–æ—á–Ω–æ–µ –∑—Ä–µ–Ω–∏–µ –≤—ã–∫–ª—é—á–µ–Ω–æ")
    end
    
    return Functions.NightVision.Enabled
end

-- 11. X-RAY (–°–ö–í–û–ó–¨ –°–¢–ï–ù–´)
local function toggleXRay()
    Functions.XRay.Enabled = not Functions.XRay.Enabled
    
    if Functions.XRay.Enabled then
        -- –î–µ–ª–∞–µ–º —Å—Ç–µ–Ω—ã –ø–æ–ª—É–ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º–∏
        for _, part in pairs(Workspace:GetDescendants()) do
            if part:IsA("BasePart") and part.Name:lower():find("wall") or 
               part.Name:lower():find("floor") or part.Name:lower():find("roof") then
                Functions.XRay.OriginalTransparency[part] = part.Transparency
                part.Transparency = 0.7
            end
        end
        print("üîç X-Ray –≤–∫–ª—é—á–µ–Ω")
    else
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å
        for part, transparency in pairs(Functions.XRay.OriginalTransparency) do
            if part and part.Parent then
                part.Transparency = transparency
            end
        end
        Functions.XRay.OriginalTransparency = {}
        print("üîç X-Ray –≤—ã–∫–ª—é—á–µ–Ω")
    end
    
    return Functions.XRay.Enabled
end

-- 12. –í–û–ó–í–†–ê–¢ –ù–ê –°–ü–ê–í–ù
local function returnToSpawn()
    local spawn = Workspace:FindFirstChild("SpawnLocation")
    if not spawn then
        spawn = Workspace:FindFirstChildOfClass("SpawnLocation")
    end
    
    local root = getRoot()
    if root and spawn then
        root.CFrame = spawn.CFrame
        print("üè† –í–æ–∑–≤—Ä–∞—â–µ–Ω –Ω–∞ —Å–ø–∞–≤–Ω")
    else
        print("‚ùå –ù–µ –Ω–∞–π–¥–µ–Ω —Å–ø–∞–≤–Ω")
    end
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ GUI (–≤–∞—à –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–π –∫–æ–¥ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç—Å—è...)
