-- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è —Ä–∞–∑–¥–µ–ª–æ–≤
local TabContainer = Instance.new("Frame")
TabContainer.Size = UDim2.new(1, -20, 0, 40)
TabContainer.Position = UDim2.new(0, 10, 0, 60)
TabContainer.BackgroundTransparency = 1
TabContainer.Parent = MainFrame

-- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è –∫–æ–Ω—Ç–µ–Ω—Ç–∞
local ContentFrame = Instance.new("Frame")
ContentFrame.Size = UDim2.new(1, -20, 0, 390)
ContentFrame.Position = UDim2.new(0, 10, 0, 110)
ContentFrame.BackgroundTransparency = 1
ContentFrame.Parent = MainFrame

-- –°–æ–∑–¥–∞–Ω–∏–µ —Ä–∞–∑–¥–µ–ª–æ–≤
local TabButtons = {}
local ContentFrames = {}
local CurrentTab = "–ö–æ–º–∞–Ω–¥—ã"

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–∫–∏ —Ä–∞–∑–¥–µ–ª–∞
local function createTabButton(tabName, index)
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 90, 1, 0)
    button.Position = UDim2.new(0, (index-1) * 95, 0, 0)
    button.BackgroundColor3 = Color3.fromRGB(50, 50, 65)
    button.TextColor3 = Color3.fromRGB(200, 200, 200)
    button.Text = tabName
    button.Font = Enum.Font.Gotham
    button.TextSize = 14
    button.AutoButtonColor = false
    button.Parent = TabContainer
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = button
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –Ω–∞–≤–µ–¥–µ–Ω–∏—è
    button.MouseEnter:Connect(function()
        if CurrentTab ~= tabName then
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(60, 60, 80)}):Play()
        end
    end)
    
    button.MouseLeave:Connect(function()
        if CurrentTab ~= tabName then
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(50, 50, 65)}):Play()
        end
    end)
    
    -- –°–æ–∑–¥–∞–Ω–∏–µ –∫–æ–Ω—Ç–µ–Ω—Ç–∞ –¥–ª—è —Ä–∞–∑–¥–µ–ª–∞
    local contentFrame = Instance.new("ScrollingFrame")
    contentFrame.Size = UDim2.new(1, 0, 1, 0)
    contentFrame.Position = UDim2.new(0, 0, 0, 0)
    contentFrame.BackgroundTransparency = 1
    contentFrame.BorderSizePixel = 0
    contentFrame.ScrollBarThickness = 4
    contentFrame.ScrollBarImageColor3 = GUI_SETTINGS.AccentColor
    contentFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    contentFrame.Visible = (tabName == CurrentTab)
    contentFrame.Parent = ContentFrame
    
    button.MouseButton1Click:Connect(function()
        -- –°–∫—Ä—ã—Ç—å –≤—Å–µ –∫–æ–Ω—Ç–µ–Ω—Ç —Ñ—Ä–µ–π–º—ã
        for _, frame in pairs(ContentFrames) do
            frame.Visible = false
        end
        
        -- –°–±—Ä–æ—Å–∏—Ç—å —Ü–≤–µ—Ç–∞ –≤—Å–µ—Ö –∫–Ω–æ–ø–æ–∫
        for name, btn in pairs(TabButtons) do
            TweenService:Create(btn, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(50, 50, 65),
                TextColor3 = Color3.fromRGB(200, 200, 200)
            }):Play()
        end
        
        -- –ü–æ–¥—Å–≤–µ—Ç–∏—Ç—å –∞–∫—Ç–∏–≤–Ω—É—é –∫–Ω–æ–ø–∫—É
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = GUI_SETTINGS.AccentColor,
            TextColor3 = Color3.fromRGB(255, 255, 255)
        }):Play()
        
        -- –ü–æ–∫–∞–∑–∞—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—ã–π –∫–æ–Ω—Ç–µ–Ω—Ç
        contentFrame.Visible = true
        CurrentTab = tabName
    end)
    
    TabButtons[tabName] = button
    ContentFrames[tabName] = contentFrame
    
    return button, contentFrame
end

-- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –∫–Ω–æ–ø–∫–∏ –≤ —Ä–∞–∑–¥–µ–ª–µ
local function createSectionButton(parent, text, description, yPosition, callback)
    local buttonFrame = Instance.new("Frame")
    buttonFrame.Size = UDim2.new(1, 0, 0, 60)
    buttonFrame.Position = UDim2.new(0, 0, 0, yPosition)
    buttonFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
    buttonFrame.BorderSizePixel = 0
    buttonFrame.Parent = parent
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 8)
    buttonCorner.Parent = buttonFrame
    
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, 0, 1, 0)
    button.BackgroundTransparency = 1
    button.Text = ""
    button.Parent = buttonFrame
    
    local buttonTitle = Instance.new("TextLabel")
    buttonTitle.Size = UDim2.new(0.7, -10, 0, 30)
    buttonTitle.Position = UDim2.new(0, 10, 0, 5)
    buttonTitle.BackgroundTransparency = 1
    buttonTitle.TextColor3 = GUI_SETTINGS.TextColor
    buttonTitle.Text = text
    buttonTitle.Font = Enum.Font.GothamBold
    buttonTitle.TextSize = 16
    buttonTitle.TextXAlignment = Enum.TextXAlignment.Left
    buttonTitle.Parent = buttonFrame
    
    local buttonDesc = Instance.new("TextLabel")
    buttonDesc.Size = UDim2.new(0.7, -10, 0, 20)
    buttonDesc.Position = UDim2.new(0, 10, 0, 35)
    buttonDesc.BackgroundTransparency = 1
    buttonDesc.TextColor3 = Color3.fromRGB(180, 180, 180)
    buttonDesc.Text = description
    buttonDesc.Font = Enum.Font.Gotham
    buttonDesc.TextSize = 12
    buttonDesc.TextXAlignment = Enum.TextXAlignment.Left
    buttonDesc.Parent = buttonFrame
    
    local buttonIndicator = Instance.new("Frame")
    buttonIndicator.Size = UDim2.new(0, 40, 0, 40)
    buttonIndicator.Position = UDim2.new(1, -50, 0.5, -20)
    buttonIndicator.BackgroundColor3 = GUI_SETTINGS.AccentColor
    buttonIndicator.Parent = buttonFrame
    
    local indicatorCorner = Instance.new("UICorner")
    indicatorCorner.CornerRadius = UDim.new(0, 8)
    indicatorCorner.Parent = buttonIndicator
    
    local indicatorText = Instance.new("TextLabel")
    indicatorText.Size = UDim2.new(1, 0, 1, 0)
    indicatorText.BackgroundTransparency = 1
    indicatorText.TextColor3 = Color3.fromRGB(255, 255, 255)
    indicatorText.Text = "‚ñ∂"
    indicatorText.Font = Enum.Font.GothamBold
    indicatorText.TextSize = 18
    indicatorText.Parent = buttonIndicator
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –Ω–∞–≤–µ–¥–µ–Ω–∏—è
    local originalColor = buttonFrame.BackgroundColor3
    
    button.MouseEnter:Connect(function()
        TweenService:Create(buttonFrame, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(55, 55, 70)
        }):Play()
        TweenService:Create(buttonIndicator, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(100, 140, 255)
        }):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(buttonFrame, TweenInfo.new(0.2), {
            BackgroundColor3 = originalColor
        }):Play()
        TweenService:Create(buttonIndicator, TweenInfo.new(0.2), {
            BackgroundColor3 = GUI_SETTINGS.AccentColor
        }):Play()
    end)
    
    -- –ù–∞–∂–∞—Ç–∏–µ –∫–Ω–æ–ø–∫–∏
    button.MouseButton1Click:Connect(function()
        -- –ê–Ω–∏–º–∞—Ü–∏—è –Ω–∞–∂–∞—Ç–∏—è
        TweenService:Create(buttonIndicator, TweenInfo.new(0.1), {
            BackgroundColor3 = Color3.fromRGB(120, 160, 255),
            Size = UDim2.new(0, 35, 0, 35)
        }):Play()
        
        TweenService:Create(buttonIndicator, TweenInfo.new(0.1), {
            BackgroundColor3 = GUI_SETTINGS.AccentColor,
            Size = UDim2.new(0, 40, 0, 40)
        }):Play()
        
        -- –í—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –æ–±—Ä–∞—Ç–Ω–æ–≥–æ –≤—ã–∑–æ–≤–∞
        if callback then
            callback()
        end
    end)
    
    return buttonFrame
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤—Å–µ—Ö —Ä–∞–∑–¥–µ–ª–æ–≤
for i, sectionName in ipairs(GUI_SETTINGS.Sections) do
    createTabButton(sectionName, i)
end

-- –ü–æ–¥—Å–≤–µ—Ç–∫–∞ –ø–µ—Ä–≤–æ–≥–æ —Ä–∞–∑–¥–µ–ª–∞
if TabButtons[CurrentTab] then
    TabButtons[CurrentTab].BackgroundColor3 = GUI_SETTINGS.AccentColor
    TabButtons[CurrentTab].TextColor3 = Color3.fromRGB(255, 255, 255)
end

-- ========== –î–û–ë–ê–í–õ–ï–ù–ò–ï –ö–ù–û–ü–û–ö –° –†–ê–ë–û–ß–ò–ú –§–£–ù–ö–¶–ò–û–ù–ê–õ–û–ú ==========

-- –†–∞–∑–¥–µ–ª "–ö–æ–º–∞–Ω–¥—ã"
local CommandsFrame = ContentFrames["–ö–æ–º–∞–Ω–¥—ã"]
if CommandsFrame then
    createSectionButton(CommandsFrame, "–¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –∏–≥—Ä–æ–∫—É", "–¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ—Ç –≤–∞—Å –∫ –≤—ã–±—Ä–∞–Ω–Ω–æ–º—É –∏–≥—Ä–æ–∫—É", 10, teleportToPlayer)
    
    createSectionButton(CommandsFrame, "–í–∫–ª—é—á–∏—Ç—å –ø–æ–ª–µ—Ç", "–ê–∫—Ç–∏–≤–∏—Ä—É–µ—Ç —Ä–µ–∂–∏–º –ø–æ–ª–µ—Ç–∞ (WASD+Space)", 80, toggleFly)
    
    createSectionButton(CommandsFrame, "–ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–π –ø—Ä—ã–∂–æ–∫", "–ü–æ–∑–≤–æ–ª—è–µ—Ç –ø—Ä—ã–≥–∞—Ç—å –≤—ã—à–µ", 150, toggleInfiniteJump)
    
    createSectionButton(CommandsFrame, "–°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã", "–£–≤–µ–ª–∏—á–∏–≤–∞–µ—Ç —Å–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏—è", 220, toggleWalkSpeed)
    
    createSectionButton(CommandsFrame, "–†–µ–∂–∏–º Noclip", "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", 290, toggleNoclip)
    
    createSectionButton(CommandsFrame, "–í–µ—Ä–Ω—É—Ç—å—Å—è –Ω–∞ —Å–ø–∞–≤–Ω", "–¢–µ–ª–µ–ø–æ—Ä—Ç –Ω–∞ —Ç–æ—á–∫—É —Å–ø–∞–≤–Ω–∞", 360, returnToSpawn)
    
    CommandsFrame.CanvasSize = UDim2.new(0, 0, 0, 430)
end

-- –†–∞–∑–¥–µ–ª "–í–∏–∑—É–∞–ª—ã"
local VisualsFrame = ContentFrames["–í–∏–∑—É–∞–ª—ã"]
if VisualsFrame then
    createSectionButton(VisualsFrame, "ESP –∏–≥—Ä–æ–∫–æ–≤", "–ü–æ–¥—Å–≤–µ—á–∏–≤–∞–µ—Ç –∏–≥—Ä–æ–∫–æ–≤ —á–µ—Ä–µ–∑ —Å—Ç–µ–Ω—ã", 10, toggleESP)
    
    createSectionButton(VisualsFrame, "–ò–º–µ–Ω–∞ –Ω–∞–¥ –≥–æ–ª–æ–≤–∞–º–∏", "–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç –∏–º–µ–Ω–∞ –∏–≥—Ä–æ–∫–æ–≤", 80, toggleNames)
    
    createSectionButton(VisualsFrame, "–¢—Ä–∞–µ–∫—Ç–æ—Ä–∏—è –ø–æ–ª–µ—Ç–∞", "–ü–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Ç—Ä–∞–µ–∫—Ç–æ—Ä–∏—é –±—Ä–æ—Å–∫–∞", 150, toggleTrajectory)
    
    createSectionButton(VisualsFrame, "–£–ª—É—á—à–µ–Ω–Ω–∞—è –≥—Ä–∞—Ñ–∏–∫–∞", "–í–∫–ª—é—á–∞–µ—Ç —É–ª—É—á—à–µ–Ω–Ω—É—é –≥—Ä–∞—Ñ–∏–∫—É", 220, toggleEnhancedGraphics)
    
    createSectionButton(VisualsFrame, "–ù–æ—á–Ω–æ–µ –∑—Ä–µ–Ω–∏–µ", "–í–∏–¥–µ—Ç—å –≤ —Ç–µ–º–Ω–æ—Ç–µ", 290, toggleNightVision)
    
    createSectionButton(VisualsFrame, "X-Ray", "–í–∏–¥–µ—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", 360, toggleXRay)
    
    VisualsFrame.CanvasSize = UDim2.new(0, 0, 0, 430)
end

-- –†–∞–∑–¥–µ–ª "–ù–∞—Å—Ç—Ä–æ–π–∫–∏"
local SettingsFrame = ContentFrames["–ù–∞—Å—Ç—Ä–æ–π–∫–∏"]
if SettingsFrame then
    -- –°–æ–∑–¥–∞–µ–º —Å–ª–∞–π–¥–µ—Ä –¥–ª—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–∫–æ—Ä–æ—Å—Ç–∏
    local speedSliderFrame = Instance.new("Frame")
    speedSliderFrame.Size = UDim2.new(1, 0, 0, 70)
    speedSliderFrame.Position = UDim2.new(0, 0, 0, 10)
    speedSliderFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
    speedSliderFrame.Parent = SettingsFrame
    
    local speedSliderCorner = Instance.new("UICorner")
    speedSliderCorner.CornerRadius = UDim.new(0, 8)
    speedSliderCorner.Parent = speedSliderFrame
    
    local speedLabel = Instance.new("TextLabel")
    speedLabel.Size = UDim2.new(1, -20, 0, 30)
    speedLabel.Position = UDim2.new(0, 10, 0, 5)
    speedLabel.BackgroundTransparency = 1
    speedLabel.TextColor3 = GUI_SETTINGS.TextColor
    speedLabel.Text = "–ú–Ω–æ–∂–∏—Ç–µ–ª—å —Å–∫–æ—Ä–æ—Å—Ç–∏: x" .. Functions.WalkSpeed.Multiplier
    speedLabel.Font = Enum.Font.Gotham
    speedLabel.TextSize = 14
    speedLabel.TextXAlignment = Enum.TextXAlignment.Left
    speedLabel.Parent = speedSliderFrame
    
    local speedSlider = Instance.new("Frame")
    speedSlider.Size = UDim2.new(1, -20, 0, 20)
    speedSlider.Position = UDim2.new(0, 10, 0, 40)
    speedSlider.BackgroundColor3 = Color3.fromRGB(60, 60, 75)
    speedSlider.Parent = speedSliderFrame
    
    local speedFill = Instance.new("Frame")
    speedFill.Size = UDim2.new(0.5, 0, 1, 0)
    speedFill.Position = UDim2.new(0, 0, 0, 0)
    speedFill.BackgroundColor3 = GUI_SETTINGS.AccentColor
    speedFill.Parent = speedSlider
    
    speedSlider.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            local connection
            connection = RunService.RenderStepped:Connect(function()
                local mousePos = UserInputService:GetMouseLocation()
                local sliderPos = speedSlider.AbsolutePosition
                local sliderSize = speedSlider.AbsoluteSize.X
                
                local relativeX = (mousePos.X - sliderPos.X) / sliderSize
                relativeX = math.clamp(relativeX, 0.1, 5)
                
                speedFill.Size = UDim2.new(relativeX/5, 0, 1, 0)
                Functions.WalkSpeed.Multiplier = math.floor(relativeX * 10) / 10
                speedLabel.Text = "–ú–Ω–æ–∂–∏—Ç–µ–ª—å —Å–∫–æ—Ä–æ—Å—Ç–∏: x" .. Functions.WalkSpeed.Multiplier
                
                -- –û–±–Ω–æ–≤–ª—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –µ—Å–ª–∏ –æ–Ω–∞ –≤–∫–ª—é—á–µ–Ω–∞
                if Functions.WalkSpeed.Enabled then
                    local humanoid = getHumanoid()
                    if humanoid then
                        humanoid.WalkSpeed = Functions.WalkSpeed.Value * Functions.WalkSpeed.Multiplier
                    end
                end
            end)
            
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    connection:Disconnect()
                end
            end)
        end
    end)
    
    createSectionButton(SettingsFrame, "–ù–∞—Å—Ç—Ä–æ–π–∫–∏ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞", "–ù–∞—Å—Ç—Ä–æ–π—Ç–µ –≤–Ω–µ—à–Ω–∏–π –≤–∏–¥ GUI", 90, function()
        print("‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –æ—Ç–∫—Ä—ã—Ç—ã")
        -- –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–∞–ª—å–Ω—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
    end)
    
    createSectionButton(SettingsFrame, "–ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏", "–ù–∞–∑–Ω–∞—á—å—Ç–µ –≥–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏", 160, function()
        print("‚å®Ô∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –≥–æ—Ä—è—á–∏—Ö –∫–ª–∞–≤–∏—à")
        -- –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –Ω–∞—Å—Ç—Ä–æ–π–∫—É –≥–æ—Ä—è—á–∏—Ö –∫–ª–∞–≤–∏—à
    end)
    
    createSectionButton(SettingsFrame, "–°–±—Ä–æ—Å –Ω–∞—Å—Ç—Ä–æ–µ–∫", "–°–±—Ä–æ—Å–∏—Ç—å –≤—Å–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏", 230, function()
        -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º –≤—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏
        if Functions.Fly.Enabled then toggleFly() end
        if Functions.ESP.Enabled then toggleESP() end
        if Functions.Names.Enabled then toggleNames() end
        if Functions.NightVision.Enabled then toggleNightVision() end
        if Functions.XRay.Enabled then toggleXRay() end
        if Functions.Noclip.Enabled then toggleNoclip() end
        if Functions.WalkSpeed.Enabled then toggleWalkSpeed() end
        if Functions.JumpPower.Enabled then toggleInfiniteJump() end
        
        -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º –º–Ω–æ–∂–∏—Ç–µ–ª–∏
        Functions.WalkSpeed.Multiplier = 3
        Functions.JumpPower.Multiplier = 2
        speedFill.Size = UDim2.new(0.6, 0, 1, 0)
        speedLabel.Text = "–ú–Ω–æ–∂–∏—Ç–µ–ª—å —Å–∫–æ—Ä–æ—Å—Ç–∏: x3.0"
        
        print("üîÑ –í—Å–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–±—Ä–æ—à–µ–Ω—ã")
    end)
    
    SettingsFrame.CanvasSize = UDim2.new(0, 0, 0, 310)
end

-- –†–∞–∑–¥–µ–ª "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"
