local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer
local mouse = player:GetMouse()

-- GUI –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local CloseButton = Instance.new("TextButton")
local TabButtons = Instance.new("Frame")
local ContentFrame = Instance.new("Frame")

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏ GUI
ScreenGui.Name = "GGH_Menu"
ScreenGui.Parent = game.CoreGui

MainFrame.Name = "MainFrame"
MainFrame.Size = UDim2.new(0, 500, 0, 400)
MainFrame.Position = UDim2.new(0.5, -250, 0.5, -200)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Parent = ScreenGui

Title.Name = "Title"
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
Title.Text = "üî• GGH ULTIMATE PACK v3.0"
Title.TextColor3 = Color3.fromRGB(255, 100, 100)
Title.TextSize = 18
Title.Font = Enum.Font.GothamBold
Title.Parent = MainFrame

CloseButton.Name = "CloseButton"
CloseButton.Size = UDim2.new(0, 30, 0, 30)
CloseButton.Position = UDim2.new(1, -30, 0, 0)
CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.white
CloseButton.TextSize = 16
CloseButton.Font = Enum.Font.GothamBold
CloseButton.Parent = MainFrame

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

-- –°–æ–∑–¥–∞–µ–º —Ç–∞–±—ã
TabButtons.Name = "TabButtons"
TabButtons.Size = UDim2.new(0, 120, 1, -30)
TabButtons.Position = UDim2.new(0, 0, 0, 30)
TabButtons.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
TabButtons.Parent = MainFrame

ContentFrame.Name = "ContentFrame"
ContentFrame.Size = UDim2.new(1, -120, 1, -30)
ContentFrame.Position = UDim2.new(0, 120, 0, 30)
ContentFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
ContentFrame.Parent = MainFrame

-- –°–ø–∏—Å–æ–∫ —Ñ—É–Ω–∫—Ü–∏–π
local tabs = {
    "üéØ –ê–ò–ú –ò –í–´–ö–ò–î",
    "üõ°Ô∏è –ê–ù–¢–ò-–ì–†–ê–ë",
    "üí• –ö–ò–ö –°–ò–°–¢–ï–ú–ê",
    "‚ö° –°–¢–ê–¢–´",
    "üîß –£–¢–ò–õ–ò–¢–´"
}

local currentTab = 1

-- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫–∏ —Ç–∞–±–æ–≤
for i, tabName in ipairs(tabs) do
    local tabButton = Instance.new("TextButton")
    tabButton.Name = "Tab"..i
    tabButton.Size = UDim2.new(1, 0, 0, 40)
    tabButton.Position = UDim2.new(0, 0, 0, (i-1)*45)
    tabButton.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    tabButton.BorderSizePixel = 0
    tabButton.Text = tabName
    tabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.Gotham
    tabButton.Parent = TabButtons
    
    tabButton.MouseButton1Click:Connect(function()
        currentTab = i
        updateContent()
    end)
end

function updateContent()
    for _, child in pairs(ContentFrame:GetChildren()) do
        child:Destroy()
    end
    
    if currentTab == 1 then
        loadAimContent()
    elseif currentTab == 2 then
        loadAntiGrabContent()
    elseif currentTab == 3 then
        loadKickContent()
    elseif currentTab == 4 then
        loadStatsContent()
    elseif currentTab == 5 then
        loadUtilsContent()
    end
end

-- ==================== [1] –ê–ò–ú –ò –í–´–ö–ò–î –°–ò–°–¢–ï–ú–ê ====================
local aimTarget = nil
local aimEnabled = false
local yeetEnabled = false
local launchPower = 500

function loadAimContent()
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.BackgroundTransparency = 1
    scroll.Parent = ContentFrame
    
    local yPos = 10
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local title = createLabel("üéØ –ê–ò–ú–ë–û–¢ –ò –í–´–ö–ò–î–´–í–ê–ù–ò–ï", 20, yPos)
    title.TextColor3 = Color3.fromRGB(255, 100, 100)
    title.Parent = scroll
    yPos = yPos + 30
    
    -- –í–∫–ª—é—á–µ–Ω–∏–µ –∞–∏–º–∞
    local aimToggle = createButton("–í–ö–õ/–í–´–ö–õ –ê–ò–ú (–ü—Ä–∞–≤–∞—è –∫–Ω–æ–ø–∫–∞ –º—ã—à–∏)", 20, yPos)
    aimToggle.Parent = scroll
    yPos = yPos + 40
    
    aimToggle.MouseButton1Click:Connect(function()
        aimEnabled = not aimEnabled
        aimToggle.Text = aimEnabled and "‚úÖ –ê–ò–ú –í–ö–õ–Æ–ß–ï–ù" or "üéØ –í–ö–õ/–í–´–ö–õ –ê–ò–ú"
        print("Aimbot: " .. (aimEnabled and "ON" or "OFF"))
    end)
    
    -- –í—ã–±–æ—Ä —Ü–µ–ª–∏
    local targetLabel = createLabel("–í—ã–±–µ—Ä–∏—Ç–µ —Ü–µ–ª—å:", 20, yPos)
    targetLabel.Parent = scroll
    yPos = yPos + 30
    
    local targetBox = Instance.new("TextBox")
    targetBox.Size = UDim2.new(0.8, 0, 0, 30)
    targetBox.Position = UDim2.new(0.1, 0, 0, yPos/400)
    targetBox.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
    targetBox.TextColor3 = Color3.white
    targetBox.Text = "–ò–º—è –∏–≥—Ä–æ–∫–∞"
    targetBox.Parent = scroll
    yPos = yPos + 40
    
    local setTargetBtn = createButton("–£—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Ü–µ–ª—å", 20, yPos)
    setTargetBtn.Parent = scroll
    yPos = yPos + 40
    
    setTargetBtn.MouseButton1Click:Connect(function()
        local targetName = targetBox.Text
        for _, plr in pairs(Players:GetPlayers()) do
            if string.lower(plr.Name) == string.lower(targetName) then
                aimTarget = plr
                print("üéØ –¶–µ–ª—å —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞: " .. plr.Name)
                break
            end
        end
    end)
    
    -- –ú–æ—â–Ω–æ—Å—Ç—å –≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏—è
    local powerLabel = createLabel("–°–∏–ª–∞ –≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏—è: " .. launchPower, 20, yPos)
    powerLabel.Parent = scroll
    yPos = yPos + 30
    
    local powerSlider = Instance.new("TextButton")
    powerSlider.Size = UDim2.new(0.8, 0, 0, 20)
    powerSlider.Position = UDim2.new(0.1, 0, 0, yPos/400)
    powerSlider.BackgroundColor3 = Color3.fromRGB(100, 100, 200)
    powerSlider.Text = "–ò–∑–º–µ–Ω–∏—Ç—å –º–æ—â–Ω–æ—Å—Ç—å"
    powerSlider.Parent = scroll
    yPos = yPos + 40
    
    powerSlider.MouseButton1Click:Connect(function()
        launchPower = launchPower + 100
        if launchPower > 2000 then launchPower = 100 end
        powerLabel.Text = "–°–∏–ª–∞ –≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏—è: " .. launchPower
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ –≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏—è
    local yeetBtn = createButton("üöÄ –í–´–ö–ò–ù–£–¢–¨ –¶–ï–õ–¨ (Y)", 20, yPos)
    yeetBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    yeetBtn.Parent = scroll
    yPos = yPos + 50
    
    yeetBtn.MouseButton1Click:Connect(function()
        yeetTarget()
    end)
    
    -- –ê–≤—Ç–æ-–≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏–µ
    local autoYeetToggle = createButton("ü§ñ –ê–í–¢–û-–í–´–ö–ò–î–´–í–ê–ù–ò–ï (T)", 20, yPos)
    autoYeetToggle.Parent = scroll
    yPos = yPos + 40
    
    autoYeetToggle.MouseButton1Click:Connect(function()
        yeetEnabled = not yeetEnabled
        autoYeetToggle.Text = yeetEnabled and "ü§ñ –ê–í–¢–û-–í–´–ö–ò–î –í–ö–õ" or "ü§ñ –ê–í–¢–û-–í–´–ö–ò–î–´–í–ê–ù–ò–ï"
        
        while yeetEnabled and aimTarget do
            yeetTarget()
            wait(0.5)
        end
    end)
    
    scroll.CanvasSize = UDim2.new(0, 0, 0, yPos + 50)
end

-- –§—É–Ω–∫—Ü–∏—è –≤—ã–∫–∏–¥—ã–≤–∞–Ω–∏—è
function yeetTarget()
    if not aimTarget or not aimTarget.Character then return end
    
    local targetRoot = aimTarget.Character:FindFirstChild("HumanoidRootPart")
    local myRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    
    if targetRoot and myRoot then
        -- –°–æ–∑–¥–∞–µ–º –º–æ—â–Ω—ã–π BodyVelocity
        local bv = Instance.new("BodyVelocity")
        bv.Velocity = Vector3.new(
            math.random(-launchPower, launchPower),
            launchPower,
            math.random(-launchPower, launchPower)
        )
        bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bv.P = 100000
        bv.Parent = targetRoot
        
        -- –î–æ–±–∞–≤–ª—è–µ–º –≤—Ä–∞—â–µ–Ω–∏–µ
        local bav = Instance.new("BodyAngularVelocity")
        bav.AngularVelocity = Vector3.new(
            math.random(-50, 50),
            math.random(-50, 50),
            math.random(-50, 50)
        )
        bav.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bav.Parent = targetRoot
        
        -- –ê–≤—Ç–æ—É–¥–∞–ª–µ–Ω–∏–µ
        game:GetService("Debris"):AddItem(bv, 3)
        game:GetService("Debris"):AddItem(bav, 3)
        
        print("üí• –í–´–ö–ò–ù–£–¢: " .. aimTarget.Name)
    end
end

-- –°–∏—Å—Ç–µ–º–∞ –∞–∏–º–±–æ—Ç–∞
mouse.Button2Down:Connect(function()
    if aimEnabled and aimTarget and aimTarget.Character then
        local targetRoot = aimTarget.Character:FindFirstChild("HumanoidRootPart")
        local myRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
        
        if targetRoot and myRoot then
            -- –ü–ª–∞–≤–Ω–æ–µ –Ω–∞–≤–µ–¥–µ–Ω–∏–µ
            local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear)
            local tween = TweenService:Create(
                workspace.CurrentCamera,
                tweenInfo,
                {CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, targetRoot.Position)}
            )
            tween:Play()
        end
    end
end)

-- ==================== [2] –ê–ù–¢–ò-–ì–†–ê–ë –°–ò–°–¢–ï–ú–ê ====================
local antiGrabEnabled = false
local antiGrabConnection

function loadAntiGrabContent()
    local yPos = 10
    
    local title = createLabel("üõ°Ô∏è –°–ò–°–¢–ï–ú–ê –ó–ê–©–ò–¢–´ –û–¢ –ì–†–ê–ë–ê", 20, yPos)
    title.TextColor3 = Color3.fromRGB(100, 200, 255)
    title.Parent = ContentFrame
    yPos = yPos + 40
    
    -- –í–∫–ª—é—á–µ–Ω–∏–µ –∑–∞—â–∏—Ç—ã
    local toggleBtn = createButton("–ê–ö–¢–ò–í–ò–†–û–í–ê–¢–¨ –ó–ê–©–ò–¢–£ (Q)", 20, yPos)
    toggleBtn.Parent = ContentFrame
    yPos = yPos + 50
    
    toggleBtn.MouseButton1Click:Connect(function()
        antiGrabEnabled = not antiGrabEnabled
        toggleBtn.Text = antiGrabEnabled and "üõ°Ô∏è –ó–ê–©–ò–¢–ê –ê–ö–¢–ò–í–ù–ê" or "–ê–ö–¢–ò–í–ò–†–û–í–ê–¢–¨ –ó–ê–©–ò–¢–£"
        
        if antiGrabEnabled then
            startAntiGrab()
        else
            stopAntiGrab()
        end
    end)
    
    -- –û–ø—Ü–∏–∏ –∑–∞—â–∏—Ç—ã
    local options = {
        {"‚úÖ –ó–∞—â–∏—Ç–∞ –æ—Ç BodyVelocity", true},
        {"‚úÖ –ó–∞—â–∏—Ç–∞ –æ—Ç BodyPosition", true},
        {"‚úÖ –ó–∞—â–∏—Ç–∞ –æ—Ç –∞–Ω–∏–º–∞—Ü–∏–π", true},
        {"‚úÖ –ó–∞—â–∏—Ç–∞ –æ—Ç —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏", true},
        {"‚úÖ –ó–∞—â–∏—Ç–∞ –æ—Ç —Å–±–∏–≤–∞–Ω–∏—è", true}
    }
    
    for i, option in ipairs(options) do
        local optionBtn = createButton(option[1], 20, yPos)
        optionBtn.Size = UDim2.new(0.9, 0, 0, 30)
        optionBtn.Parent = ContentFrame
        yPos = yPos + 35
    end
    
    -- –°—Ç–∞—Ç—É—Å –∑–∞—â–∏—Ç—ã
    local statusLabel = createLabel("–°—Ç–∞—Ç—É—Å: –í–´–ö–õ", 20, yPos)
    statusLabel.Parent = ContentFrame
    yPos = yPos + 30
    
    function updateStatus()
        statusLabel.Text = "–°—Ç–∞—Ç—É—Å: " .. (antiGrabEnabled and "üü¢ –ê–ö–¢–ò–í–ù–ê" or "üî¥ –í–´–ö–õ")
    end
end

function startAntiGrab()
    antiGrabConnection = RunService.Heartbeat:Connect(function()
        local character = player.Character
        if not character then return end
        
        -- –£–¥–∞–ª—è–µ–º –≤—Å–µ —Å–∏–ª—ã
        for _, obj in pairs(character:GetDescendants()) do
            if obj:IsA("BodyVelocity") or obj:IsA("BodyPosition") or 
               obj:IsA("BodyAngularVelocity") or obj:IsA("BodyForce") then
                obj:Destroy()
            end
        end
        
        -- –ó–∞—â–∏—Ç–∞ –æ—Ç –∞–Ω–∏–º–∞—Ü–∏–π
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
                track:Stop()
            end
        end
        
        -- –§–∏–∫—Å–∏—Ä—É–µ–º –ø–æ–∑–∏—Ü–∏—é (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
        local root = character:FindFirstChild("HumanoidRootPart")
        if root then
            root.Velocity = Vector3.new(root.Velocity.X, math.clamp(root.Velocity.Y, -50, 50), root.Velocity.Z)
        end
    end)
end

function stopAntiGrab()
    if antiGrabConnection then
        antiGrabConnection:Disconnect()
        antiGrabConnection = nil
    end
end

-- ==================== [3] –ö–ò–ö –°–ò–°–¢–ï–ú–ê ====================
function loadKickContent()
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.BackgroundTransparency = 1
    scroll.Parent = ContentFrame
    
    local yPos = 10
    
    local title = createLabel("üí• –ú–û–©–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ö–ò–ö–ê", 20, yPos)
    title.TextColor3 = Color3.fromRGB(255, 150, 50)
    title.Parent = scroll
    yPos = yPos + 40
    
    -- –ü–æ–ª–µ –¥–ª—è –≤–≤–æ–¥–∞ –Ω–∏–∫–∞
    local nameBox = Instance.new("TextBox")
    nameBox.Size = UDim2.new(0.8, 0, 0, 35)
    nameBox.Position = UDim2.new(0.1, 0, 0, yPos/400)
    nameBox.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
    nameBox.TextColor3 = Color3.white
    nameBox.Text = "–í–≤–µ–¥–∏—Ç–µ –Ω–∏–∫ –∏–≥—Ä–æ–∫–∞"
    nameBox.Parent = scroll
    yPos = yPos + 45
    
    -- –ú–µ—Ç–æ–¥—ã –∫–∏–∫–∞
    local kickMethods = {
        {"üöÄ –ö–ò–ö –í –ù–ï–ë–û", function(target) kickToSky(target) end},
        {"üåÄ –ö–ò–ö –í –ë–ï–ó–î–ù–£", function(target) kickToVoid(target) end},
        {"üí´ –°–ü–ò–ù-–ö–ò–ö", function(target) spinKick(target) end},
        {"üî• –ö–ò–ö –° –í–ó–†–´–í–û–ú", function(target) explodeKick(target) end},
        {"‚ö° –ú–û–õ–ù–ò–ï–í–´–ô –ö–ò–ö", function(target) lightningKick(target) end},
        {"‚ùÑÔ∏è –õ–ï–î–Ø–ù–û–ô –ö–ò–ö", function(target) freezeKick(target) end}
    }
    
    for i, method in ipairs(kickMethods) do
        local btn = createButton(method[1], 20, yPos)
        btn.Size = UDim2.new(0.9, 0, 0, 35)
        btn.Parent = scroll
        
        btn.MouseButton1Click:Connect(function()
            local targetName = nameBox.Text
            for _, plr in pairs(Players:GetPlayers()) do
                if string.find(string.lower(plr.Name), string.lower(targetName)) then
                    method[2](plr)
                    break
                end
            end
        end)
        
        yPos = yPos + 40
    end
    
    -- –ú–∞—Å—Å–æ–≤—ã–π –∫–∏–∫ –≤—Å–µ—Ö
    local massKickBtn = createButton("‚ò¢Ô∏è –ö–ò–ö–ù–£–¢–¨ –í–°–ï–• –ò–ì–†–û–ö–û–í", 20, yPos)
    massKickBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    massKickBtn.Parent = scroll
    yPos = yPos + 50
    
    massKickBtn.MouseButton1Click:Connect(function()
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= player then
                kickToSky(plr)
                wait(0.1)
            end
        end
    end)
    
    scroll.CanvasSize = UDim2.new(0, 0, 0, yPos + 50)
end

-- –ú–µ—Ç–æ–¥—ã –∫–∏–∫–∞
function kickToSky(target)
    if not target.Character then return end
    local root = target.Character:FindFirstChild("HumanoidRootPart")
    if root then
        root.CFrame = root.CFrame * CFrame.new(0, 10000, 0)
    end
end

function kickToVoid(target)
    if not target.Character then return end
    local root = target.Character:FindFirstChild("HumanoidRootPart")
    if root then
        root.CFrame = CFrame.new(0, -1000, 0)
    end
end

function spinKick(target)
    if not target.Character then return end
    local root = target.Character:FindFirstChild("HumanoidRootPart")
    if root then
        local bav = Instance.new("BodyAngularVelocity")
        bav.AngularVelocity = Vector3.new(0, 100, 0)
        bav.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bav.Parent = root
        
        local bv = Instance.new("BodyVelocity")
        bv.Velocity = Vector3.new(0, 100, 0)
        bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bv.Parent = root
        
        game.Debris:AddItem(bav, 5)
        game.Debris:AddItem(bv, 5)
    end
end

-- ==================== [4] –°–ò–°–¢–ï–ú–ê –°–¢–ê–¢–û–í ====================
local walkSpeed = 16
local jumpPower = 50

function loadStatsContent()
    local yPos = 10
    
    local title = createLabel("‚ö° –ò–ó–ú–ï–ù–ï–ù–ò–ï –•–ê–†–ê–ö–¢–ï–†–ò–°–¢–ò–ö", 20, yPos)
    title.TextColor3 = Color3.fromRGB(100, 255, 100)
    title.Parent = ContentFrame
    yPos = yPos + 40
    
    -- –°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã
    local speedLabel = createLabel("–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkSpeed, 20, yPos)
    speedLabel.Parent = ContentFrame
    yPos = yPos + 30
    
    local speedPlus = createButton("+", 20, yPos)
    speedPlus.Size = UDim2.new(0, 40, 0, 30)
    speedPlus.Parent = ContentFrame
    
    local speedMinus = createButton("-", 70, yPos)
    speedMinus.Size = UDim2.new(0, 40, 0, 30)
    speedMinus.Parent = ContentFrame
    
    local speedMax = createButton("MAX", 120, yPos)
    speedMax.Size = UDim2.new(0, 60, 0, 30)
    speedMax.Parent = ContentFrame
    yPos = yPos + 40
    
    speedPlus.MouseButton1Click:Connect(function()
        walkSpeed = walkSpeed + 10
        updateStats()
        speedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkSpeed
    end)
    
    speedMinus.MouseButton1Click:Connect(function()
        walkSpeed = math.max(16, walkSpeed - 10)
        updateStats()
        speedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkSpeed
    end)
    
    speedMax.MouseButton1Click:Connect(function()
        walkSpeed = 500
        updateStats()
        speedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: MAX"
    end)
    
    -- –°–∏–ª–∞ –ø—Ä—ã–∂–∫–∞
    local jumpLabel = createLabel("–ü—Ä—ã–∂–æ–∫: " .. jumpPower, 20, yPos)
    jumpLabel.Parent = ContentFrame
    yPos = yPos + 30
    
    local jumpPlus = createButton("+", 20, yPos)
    jumpPlus.Size = UDim2.new(0, 40, 0, 30)
    jumpPlus.Parent = ContentFrame
    
    local jumpMinus = createButton("-", 70, yPos)
    jumpMinus.Size = UDim2.new(0, 40, 0, 30)
    jumpMinus.Parent = ContentFrame
    
    local jumpMax = createButton("MAX", 120, yPos)
    jumpMax.Size = UDim2.new(0, 60, 0, 30)
    jumpMax.Parent = ContentFrame
    yPos = yPos + 40
    
    jumpPlus.MouseButton1Click:Connect(function()
        jumpPower = jumpPower + 25
        updateStats()
        jumpLabel.Text = "–ü—Ä—ã–∂–æ–∫: " .. jumpPower
    end)
    
    jumpMinus.MouseButton1Click:Connect(function()
        jumpPower = math.max(50, jumpPower - 25)
        updateStats()
        jumpLabel.Text = "–ü—Ä—ã–∂–æ–∫: " .. jumpPower
    end)
    
    jumpMax.MouseButton1Click:Connect(function()
        jumpPower = 1000
        updateStats()
        jumpLabel.Text = "–ü—Ä—ã–∂–æ–∫: MAX"
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ —Å–±—Ä–æ—Å–∞
    local resetBtn = createButton("üîÑ –°–ë–†–û–° –°–¢–ê–¢–û–í", 20, yPos)
    resetBtn.Parent = ContentFrame
    yPos = yPos + 50
    
    resetBtn.MouseButton1Click:Connect(function()
        walkSpeed = 16
        jumpPower = 50
        updateStats()
        speedLabel.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkSpeed
        jumpLabel.Text = "–ü—Ä—ã–∂–æ–∫: " .. jumpPower
    end)
end

function updateStats()
    local character = player.Character
    if not character then return end
    
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = walkSpeed
        humanoid.JumpPower = jumpPower
    end
end

-- ==================== [5] –£–¢–ò–õ–ò–¢–´ ====================
function loadUtilsContent()
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.BackgroundTransparency = 1
    scroll.Parent = ContentFrame
    
    local yPos = 10
    
    local title = createLabel("üîß –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –£–¢–ò–õ–ò–¢–´", 20, yPos)
    title.TextColor3 = Color3.fromRGB(200, 150, 255)
    title.Parent = scroll
    yPos = yPos + 40
    
    -- –£—Ç–∏–ª–∏—Ç—ã
    local utils = {
        {"üì¶ NO-CLIP (N)", function() toggleNoclip() end},
        {"üëª INVISIBLE", function() makeInvisible() end},
        {"üíæ TP TO SPAWN", function() teleportToSpawn() end},
        {"üéØ TP TO MOUSE", function() teleportToMouse() end},
        {"üîç ESP –ò–ì–†–û–ö–û–í", function() toggleESP() end},
        {"‚öôÔ∏è FIX CAMERA", function() fixCamera() end},
        {"üåÄ ANTI-AFK", function() toggleAntiAFK() end},
        {"‚ú® CLEAN MAP", function() cleanMap() end}
    }
    
    for i, util in ipairs(utils) do
        local btn = createButton(util[1], 20, yPos)
        btn.Size = UDim2.new(0.9, 0, 0, 35)
        btn.Parent = scroll
        
        btn.MouseButton1Click:Connect(util[2])
        
        yPos = yPos + 40
    end
    
    -- –ö–ª–∞–≤–∏—à–∏ –±—ã—Å—Ç—Ä–æ–≥–æ –¥–æ—Å—Ç—É–ø–∞
    yPos = yPos + 20
    local hotkeysTitle = createLabel("üéÆ –ì–û–†–Ø–ß–ò–ï –ö–õ–ê–í–ò–®–ò:", 20, yPos)
    hotkeysTitle.Parent = scroll
    yPos = yPos + 30
    
    local hotkeys = {
        "Q - –ó–∞—â–∏—Ç–∞ –æ—Ç –≥—Ä–∞–±–∞",
        "Y - –í—ã–∫–∏–Ω—É—Ç—å —Ü–µ–ª—å",
        "T - –ê–≤—Ç–æ-–≤—ã–∫–∏–¥",
        "N - No-Clip",
        "R - –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –º—ã—à–∏",
        "F - –õ–µ—Ç–∞—Ç—å (—É–¥–µ—Ä–∂–∏–≤–∞—Ç—å)"
    }
    
    for i, hotkey in ipairs(hotkeys) do
        local label = createLabel(hotkey, 20, yPos)
        label.TextXAlignment = Enum.TextXAlignment.Left
        label.Parent = scroll
        yPos = yPos + 25
    end
    
    scroll.CanvasSize = UDim2.new(0, 0, 0, yPos + 50)
end

-- –§—É–Ω–∫—Ü–∏–∏ —É—Ç–∏–ª–∏—Ç
function toggleNoclip()
    local character = player.Character
    if not character then return end
    
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = not part.CanCollide
        end
    end
    print("No-Clip: " .. (character.Head.CanCollide and "OFF" or "ON"))
end

function teleportToMouse()
    local character = player.Character
    if not character then return end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if root then
        root.CFrame = CFrame.new(mouse.Hit.Position) + Vector3.new(0, 5, 0)
    end
end

-- ==================== –í–°–ü–û–ú–û–ì–ê–¢–ï–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò ====================
function createButton(text, x, y)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.9, 0, 0, 40)
    btn.Position = UDim2.new(0.05, 0, 0, y/400)
    btn.BackgroundColor3 = Color3.fromRGB(70, 70, 90)
    btn.TextColor3 = Color3.white
    btn.Text = text
    btn.TextSize = 14
    btn.Font = Enum.Font.Gotham
    return btn
end

function createLabel(text, x, y)
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(0.9, 0, 0, 30)
    label.Position = UDim2.new(0.05, 0, 0, y/400)
    label.BackgroundTransparency = 1
    label.TextColor3 = Color3.white
    label.Text = text
    label.TextSize = 16
    label.Font = Enum.Font.Gotham
    return label
end

-- ==================== –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø ====================
updateContent()

-- –ù–∞–∑–Ω–∞—á–µ–Ω–∏–µ –≥–æ—Ä—è—á–∏—Ö –∫–ª–∞–≤–∏—à
UIS.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.Q then
        antiGrabEnabled = not antiGrabEnabled
        if antiGrabEnabled then startAntiGrab() else stopAntiGrab() end
        print("Anti-Grab: " .. (antiGrabEnabled and "ON" : "OFF"))
        
    elseif input.KeyCode == Enum.KeyCode.Y then
        yeetTarget()
        
    elseif input.KeyCode == Enum.KeyCode.N then
        toggleNoclip()
        
    elseif input.KeyCode == Enum.KeyCode.R then
        teleportToMouse()
    end
end)

-- –ê–≤—Ç–æ-–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–æ–≤ –ø—Ä–∏ —Å–º–µ–Ω–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
player.CharacterAdded:Connect(function()
    wait(1)
    updateStats()
end)

