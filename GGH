-- Status Label
local StatusLabel = Instance.new("TextLabel")
StatusLabel.Size = UDim2.new(1, -20, 0, 30)
StatusLabel.Position = UDim2.new(0, 10, 1, -40)
StatusLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
StatusLabel.Text = "Status: Ready"
StatusLabel.Font = Enum.Font.Gotham
StatusLabel.TextSize = 14
StatusLabel.Parent = MainFrame

-- Variables
local AntiGrabEnabled = true
local FixedParts = {}
local SelectedParts = {}

-- Functions
local function updateStatus(text)
    StatusLabel.Text = "Status: " .. text
end

local function kickPlayer(playerName)
    local targetPlayer = Players:FindFirstChild(playerName)
    if targetPlayer and targetPlayer.Character then
        updateStatus("Kicking " .. playerName .. "...")
        
        -- Method 1: Character destruction
        for _, part in pairs(targetPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part:Destroy()
            end
        end
        
        -- Method 2: Network ownership manipulation
        local hum = targetPlayer.Character:FindFirstChild("Humanoid")
        if hum then
            hum:ChangeState(Enum.HumanoidStateType.Physics)
        end
        
        updateStatus(playerName .. " kicked!")
        task.wait(1)
        updateStatus("Ready")
    else
        updateStatus("Player not found!")
    end
end

local function antiGrab()
    if not AntiGrabEnabled then return end
    
    local char = LocalPlayer.Character
    if not char then return end
    
    local root = char:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    -- Anti-grab techniques
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local targetRoot = player.Character:FindFirstChild("HumanoidRootPart")
            if targetRoot then
                local distance = (root.Position - targetRoot.Position).Magnitude
                
                if distance < 10 then -- If someone is too close
                    -- Push away
                    local direction = (root.Position - targetRoot.Position).Unit
                    root.Velocity = direction * 100
                    
                    -- Anchor character
                    root.Anchored = true
                    task.wait(0.1)
                    root.Anchored = false
                end
            end
        end
    end
end

local function updatePartsList()
    PartsList:ClearAllChildren()
    SelectedParts = {}
    
    local yPos = 0
    local char = LocalPlayer.Character
    if char then
        for _, part in pairs(char:GetChildren()) do
            if part:IsA("BasePart") then
                local partFrame = Instance.new("Frame")
                partFrame.Size = UDim2.new(1, 0, 0, 25)
                partFrame.Position = UDim2.new(0, 0, 0, yPos)
                partFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
                partFrame.BorderSizePixel = 0
                
                local partName = Instance.new("TextLabel")
                partName.Size = UDim2.new(0.8, 0, 1, 0)
                partName.Position = UDim2.new(0, 5, 0, 0)
                partName.BackgroundTransparency = 1
                partName.TextColor3 = Color3.fromRGB(255, 255, 255)
                partName.Text = part.Name
                partName.Font = Enum.Font.Gotham
                partName.TextSize = 12
                partName.TextXAlignment = Enum.TextXAlignment.Left
                partName.Parent = partFrame
                
                local selectButton = Instance.new("TextButton")
                selectButton.Size = UDim2.new(0.2, -5, 0.8, 0)
                selectButton.Position = UDim2.new(0.8, 0, 0.1, 0)
                selectButton.BackgroundColor3 = Color3.fromRGB(70, 70, 90)
                selectButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                selectButton.Text = "Select"
                selectButton.Font = Enum.Font.Gotham
                selectButton.TextSize = 11
                selectButton.Parent = partFrame
                
                selectButton.MouseButton1Click:Connect(function()
                    if SelectedParts[part] then
                        SelectedParts[part] = nil
                        selectButton.BackgroundColor3 = Color3.fromRGB(70, 70, 90)
                        selectButton.Text = "Select"
                    else
                        SelectedParts[part] = true
                        selectButton.BackgroundColor3 = Color3.fromRGB(80, 150, 255)
                        selectButton.Text = "Selected"
                    end
                end)
                
                partFrame.Parent = PartsList
                yPos = yPos + 30
            end
        end
    end
    
    PartsList.CanvasSize = UDim2.new(0, 0, 0, yPos)
end

local function fixSelectedParts()
    for part, _ in pairs(SelectedParts) do
        if part and part.Parent then
            -- Make part unanchored but controlled
            part.Anchored = false
            part.CanCollide = true
            part.Massless = false
            
            -- Add to fixed parts list
            FixedParts[part] = {
                OriginalCFrame = part.CFrame,
                Part = part
            }
            
            updateStatus("Fixed " .. part.Name)
        end
    end
end

-- Connections
KickButton.MouseButton1Click:Connect(function()
    local playerName = PlayerDropdown.Text
    if playerName ~= "" then
        kickPlayer(playerName)
    end
end)

AntiGrabToggle.MouseButton1Click:Connect(function()
    AntiGrabEnabled = not AntiGrabEnabled
    if AntiGrabEnabled then
        AntiGrabToggle.BackgroundColor3 = Color3.fromRGB(60, 150, 80)
        AntiGrabToggle.Text = "ANTI-GRAB: ON"
        updateStatus("Anti-Grab Enabled")
    else
        AntiGrabToggle.BackgroundColor3 = Color3.fromRGB(150, 60, 60)
        AntiGrabToggle.Text = "ANTI-GRAB: OFF"
        updateStatus("Anti-Grab Disabled")
    end
end)

FixPartButton.MouseButton1Click:Connect(function()
    fixSelectedParts()
end)

CombatButton.MouseButton1Click:Connect(function()
    CombatFrame.Visible = true
    VisualsFrame.Visible = false
    CombatButton.BackgroundColor3 = Color3.fromRGB(80, 80, 100)
    VisualsButton.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
end)

VisualsButton.MouseButton1Click:Connect(function()
    CombatFrame.Visible = false
    VisualsFrame.Visible = true
    VisualsButton.BackgroundColor3 = Color3.fromRGB(80, 80, 100)
    CombatButton.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
end)

-- Update parts list periodically
game:GetService("RunService").Heartbeat:Connect(function()
    updatePartsList()
end)

-- Anti-grab loop
spawn(function()
    while true do
        if AntiGrabEnabled then
            antiGrab()
        end
        task.wait(0.1)
    end
end)

-- Fix parts position loop
spawn(function()
    while true do
        for _, data in pairs(FixedParts) do
            if data.Part and data.Part.Parent then
                data.Part.CFrame = data.OriginalCFrame
            end
        end
        task.wait()
    end
end)

-- Auto-populate player names
spawn(function()
    while true do
        local players = Players:GetPlayers()
        local playerNames = {}
        for _, player in pairs(players) do
            if player ~= LocalPlayer then
                table.insert(playerNames, player.Name)
            end
        end
        
        if #playerNames > 0 then
            PlayerDropdown.PlaceholderText = "Players: " .. table.concat(playerNames, ", ")
        end
        task.wait(5)
    end
end)

-- Initialization
updateStatus("GGH Script Loaded!")
updatePartsList()

warn("GGH Script by Xeno Exploiter loaded successfully!")
